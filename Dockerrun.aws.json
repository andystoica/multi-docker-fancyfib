{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "fancyfib-client",
      "image": "andystoica/fancybit-client",
      "hostname": "client",
      "essential": false,
      "memory": 256
    },
    {
      "name": "fancyfib-api",
      "image": "andystoica/fancybit-api",
      "hostname": "api",
      "essential": false,
      "memory": 256
    },
    {
      "name": "fancyfib-worker",
      "image": "andystoica/fancyfib-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 256
    },
    {
      "name": "fancyfib-router",
      "image": "andystoica/fancyfib-router",
      "hostname": "router",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["fancyfib-client", "fancyfib-api"],
      "memory": 256
    }
  ]
}
